    # Underwater operations
    underwater = await marine_bot.perform_underwater_maintenance(
        depth_meters=100,
        tasks=[
            "pipeline_inspection",
            "structure_welding",
            "cathodic_protection_check",
            "marine_growth_removal"
        ],
        equipment=["ROV", "underwater_welding_system", "cleaning_tools"],
        dive_time_hours=8
    )
    
    return {"maintenance": maintenance, "offshore": offshore, "underwater": underwater}
