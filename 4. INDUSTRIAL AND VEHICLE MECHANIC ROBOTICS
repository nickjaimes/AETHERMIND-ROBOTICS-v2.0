class MechanicalSystems:
    """Comprehensive mechanical systems expertise"""
    
    capabilities = [
        "engine_diagnostics",          # ICE and electric motor analysis
        "transmission_repair",         # Gearbox and drive train systems
        "hydraulics_maintenance",      # Hydraulic system service
        "pneumatics_servicing",        # Compressed air systems
        "bearing_replacement",         # Precision bearing installation
        "alignment_services",          # Shaft and component alignment
        "vibration_analysis",          # Mechanical vibration diagnostics
        "thermal_imaging_analysis"     # Heat pattern analysis
    ]
